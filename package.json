	"keywords": [
		"lambda-functions",
		"lambda-edge",
		"serverless",
		"s3-website",
		"cloudfront",
		"prerendering",
		"seo",
		"dynamic-rendering"
	],
	"version": "1.0.3",
	"main": "src/prerenderer/prerenderer.js",
	"author": "Mikael Vesavuori",
	"repository": {
		"type": "git",
		"url": "https://github.com/mikaelvesavuori/lambda-dynamic-prerenderer"
	},
	"license": "MIT",
	"scripts": {
		"deploy": "serverless deploy",
		"deploy:edge": "serverless deploy --config serverless.edge.yml"
	},
	"dependencies": {
		"chrome-aws-lambda": "5.3.1",
		"puppeteer-core": "5.3.1"
	},
	"devDependencies": {
		"@babel/core": "^7.12.0",
		"@babel/preset-env": "^7.12.0",
		"babel-loader": "^8.1.0",
		"serverless-webpack": "^5.3.5",
		"webpack": "^5.1.0"
	}
}
